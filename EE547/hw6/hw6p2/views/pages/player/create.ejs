<div id="flash" style="display: none;">
  <div>
    Messages
  </div>
  <div id="flash-message">
  </div>
</div>
<h1>Create Player</h1>
<form id="player-form">
  <div>
    <label>First Name: <input type="text" id="fname" name="fname"></label>
  </div>
  <div>
    <label>Last Name: <input type="text" id="lname" name="lname"></label>
  </div>
  <div>
    <label>Handed: <select id="handed" name="handed">
        <option value="left">Left</option>
        <option value="right">Right</option>
        <option value="ambi">Ambi</option>
      </select></label>
  </div>
  <div>
    <label>Initial Balance USD Cents: <input type="text" id="initial_balance_usd_cents"
        name="initial_balance_usd_cents"></label>
  </div>
  <!-- <input type="hidden" name="redirect" value="/players.html"> -->
  <div>
    <button type="submit" onclick="createCheck()">Submit</button>
  </div>
</form>
<script>
  document.addEventListener('DOMContentLoaded', () => {
    const form = document.getElementById('player-form')
    form.addEventListener('submit', (event) => {
      event.preventDefault()
    })
  })
</script>